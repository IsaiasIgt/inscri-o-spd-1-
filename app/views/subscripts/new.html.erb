<h1 id= "h1">INSCRIÇÃO SUPER DESENVOLVEDOR</h1>
<h2 id= "h2"> Seu futuro esta aqui</h2>
<div id= "form">
  <%= form_for @subscript do |f| %><br />
  <%= f.text_field :name, placeholder: "Nome" %><br />
    <%= f.text_field :email, placeholder: "E-mail" %><br />
      <%= f.text_field :phone, placeholder:"Telefone" %><br />
        <%= f.text_area :description, placeholder: "Descrição" %><br />
        <%= f.select :language, ["Javascript", "Python", "Java", "Php", "Ruby"], include_blank: "Linguagem" %><br />
          <%= f.select :schooling, ["Fundamental", "Médio", "Superior Incompleto", "Superior Completo"], include_black: "Escolaridade"%><br />
            <%= f.submit "enviar"%>
              <% end %>
</div>